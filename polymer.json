{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/pages/app-home.html",
    "src/pages/app-login.html",
    "src/pages/app-register.html",
    "src/pages/app-goodies.html",
    "src/pages/app-category.html",
    "src/pages/app-user.html",
    "src/pages/app-me.html",
    "src/pages/app-cart.html",
    "src/components/app-me-account/app-me-account.html",
    "src/components/app-me-goodies/app-me-goodies.html",
    "src/components/app-goodies-editor/app-goodies-editor.html",
    "src/components/app-me-store/app-me-store.html",
    "src/components/app-me-earnings/app-me-earnings.html",
    "src/components/app-dashboard/app-dashboard.html"

  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
    "node_modules/redux/dist/redux.min.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
   "builds": [{
     "bundle": true,
    "name": "es5-unbundled",
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "addPushManifest": true
  },{
    "name": "es6-unbundled",
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": false,
    "addPushManifest": false
  }]
}
